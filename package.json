{
  "name": "accubooks-enterprise",
  "version": "1.0.0",
  "description": "Enterprise-grade accounting software with QuickBooks-like features including invoicing, payroll, inventory, and financial reporting",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "postinstall": "cd client && npm install && npm rebuild rollup --force && cd .. && prisma generate",
    "ci:production-check": "./scripts/ci-production-check.sh",
    "migration:safety": "./scripts/migration-safety-ci.sh",
    "migration:validate": "node -r ts-node/register server/migration/migration-validator.ts",
    "migration:rollback-check": "node -r ts-node/register server/migration/rollback-safety.ts",
    "migration:drift-check": "node -r ts-node/register server/migration/schema-drift-detector.ts",
    "migration:install-hooks": "./scripts/migration-safety-hook.sh --install",
    "migration:uninstall-hooks": "./scripts/migration-safety-hook.sh --uninstall",
    "build": "vite build",
    "build:worker": "tsc --project tsconfig.worker.json",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:server": "cross-env NODE_ENV=production npx tsc --project tsconfig.server.json",
    "build:backend": "cross-env NODE_ENV=production npx tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec tsx server/index.ts",
    "start:worker": "cross-env NODE_ENV=production node dist/worker/index.js",
    "start:all": "concurrently \"npm run start:dev\" \"npm run start:worker\"",
    "db:push": "drizzle-kit push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "migrate:production": "drizzle-kit push --config=drizzle.config.ts",
    "test:smoke:staging": "cross-env NODE_ENV=staging vitest run --project server --testNamePattern='smoke'",
    "test:smoke:production": "cross-env NODE_ENV=production vitest run --project server --testNamePattern='smoke'",
    "test:concurrent": "vitest run server/observability/__tests__/stress-tests/idempotent-write-stress.test.ts",
    "test:multi-tenant": "vitest run server/observability/__tests__/stress-tests/idempotent-write-stress.test.ts",
    "benchmark:production": "node scripts/benchmark-production.mjs",
    "reports:weekly:staging": "node scripts/generate-weekly-report.mjs --env=staging",
    "test": "vitest run",
    "test:backend": "vitest run --project server",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:integration": "cross-env NODE_ENV=test jest --config=jest.config.cjs --testMatch='**/*.test.ts'",
    "test:watch:integration": "cross-env NODE_ENV=test jest --watch --config=jest.config.cjs",
    "test:coverage:integration": "cross-env NODE_ENV=test jest --coverage --config=jest.config.cjs",
    "lint": "node ./node_modules/eslint/bin/eslint.js --no-eslintrc --config ./.eslintrc.cjs server shared src/runtime src/types client/src --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "validate": "npm run lint && npm run format:check && npm run build",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rimraf dist coverage .next .turbo node_modules",
    "reset": "npm run clean && npm install",
    "prepare": "husky install",
    "verify": "powershell -ExecutionPolicy Bypass -File .\\verify-all.ps1",
    "verify:env": "node scripts/verify-env.mjs",
    "verify:types": "tsc --noEmit",
    "verify:type-lock": "node -e \"const v=require('typescript').version; if(v!=='5.3.3'){console.error('TypeScript version drift: '+v); process.exit(1)}; console.log('TypeScript locked at', v)\"",
    "verify:contract-lock": "node scripts/check-contract-lock.mjs",
    "relock:contract-lock": "node scripts/relock-contract-lock.mjs",
    "verify:release-governance": "node scripts/check-release-governance.mjs",
    "verify:api-version-governance": "node scripts/check-api-version-governance.mjs",
    "relock:api-version-locks": "node scripts/relock-api-version-locks.mjs",
    "verify:runtime-invariants": "node scripts/verify-runtime-invariants.mjs",
    "verify:prod-readiness": "node scripts/verify-prod-readiness.mjs",
    "verify:prereqs": "node scripts/verify-prereqs.mjs",
    "verify:chaos": "node scripts/verify-chaos.mjs",
    "verify:slo": "node --import tsx scripts/verify-slo.mjs",
    "verify:disaster-recovery": "node scripts/verify-disaster-recovery.mjs",
    "verify:security": "node scripts/verify-security.mjs",
    "verify:enterprise-assurance": "node scripts/verify-enterprise-assurance.mjs",
    "verify:public-trust": "node scripts/verify-public-trust.mjs",
    "verify:governance-status": "node scripts/verify-governance-status.mjs",
    "verify:governance-history-chain": "node scripts/verify-governance-history-chain.mjs",
    "verify:phase4": "node scripts/verify-phase4.mjs",
    "verify:release-guard": "node scripts/verify-release-guard.mjs",
    "verify:test-metrics": "node scripts/run-tests-with-metrics.mjs",
    "relock:test-metrics-baseline": "cross-env UPDATE_TEST_METRICS_BASELINE=true node scripts/run-tests-with-metrics.mjs",
    "verify:test-stubs-tracked": "node scripts/verify-test-stubs-tracked.mjs",
    "verify:artifact-denylist": "node scripts/verify-artifact-denylist.mjs",
    "verify:workflow-scripts": "node scripts/verify-workflow-scripts.mjs",
    "verify:observability-invariants": "node scripts/verify-observability-invariants.mjs",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "health": "node healthcheck.js"
  },
